CSSP:Requirement
  rdf:type owl:Class ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object CSSP:AbstractRequirement ;
            sp:predicate rdf:type ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasClause ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:TriplePath ;
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:path [
                rdf:type sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 1 ;
                sp:subPath <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToAttribute> ;
              ] ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object owl:Class ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object spin:_this ;
            sp:predicate CSSP:hasRequirement ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasClause ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:TriplePath ;
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:path [
                rdf:type sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 1 ;
                sp:subPath <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToAttribute> ;
              ] ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object <http://lpis.csd.auth.gr/ontologies/2015/RBLP#Entity> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object spin:_this ;
            sp:predicate CSSP:isSubjectofRequirement ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasMain ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:predicate <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToSubject> ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object <http://lpis.csd.auth.gr/ontologies/2015/RBLP#SimpleEntity> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
