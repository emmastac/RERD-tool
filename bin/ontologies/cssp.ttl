CSSP:AbstractRequirement
  rdf:type owl:Class ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasAbstractEntity ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasClause ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:TriplePath ;
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:path [
                rdf:type sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 1 ;
                sp:subPath <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToAttribute> ;
              ] ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object owl:Class ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:predicate CSSP:mainAbstractEntity ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasMain ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:predicate <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToSubject> ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object owl:Class ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  spin:rule [
      rdf:type sp:Construct ;
      sp:templates (
          [
            sp:object [
                sp:varName "e1"^^xsd:string ;
              ] ;
            sp:predicate CSSP:mainAbstractEntity ;
            sp:subject spin:_this ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "x"^^xsd:string ;
              ] ;
            sp:predicate CSSP:hasMain ;
            sp:subject spin:_this ;
          ]
          [
            sp:object [
                sp:varName "e"^^xsd:string ;
              ] ;
            sp:predicate <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToSubject> ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            rdf:type sp:NotExists ;
            sp:elements (
                [
                  sp:object owl:Class ;
                  sp:predicate rdf:type ;
                  sp:subject [
                      sp:varName "e"^^xsd:string ;
                    ] ;
                ]
              ) ;
          ]
          [
            rdf:type sp:TriplePath ;
            sp:object [
                sp:varName "e1"^^xsd:string ;
              ] ;
            sp:path [
                rdf:type sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 1 ;
                sp:subPath <http://lpis.csd.auth.gr/ontologies/2015/RBLP#isRelatedToAttribute> ;
              ] ;
            sp:subject [
                sp:varName "x"^^xsd:string ;
              ] ;
          ]
          [
            sp:object owl:Class ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "e1"^^xsd:string ;
              ] ;
          ]
        ) ;
    ] ;
  rdfs:label "Abstract req"^^xsd:string ;
  rdfs:subClassOf CSSP:Requirement ;
.
